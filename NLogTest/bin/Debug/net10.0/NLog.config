<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" 
	  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<targets>
		<target xsi:type="File" name="FileJson" fileName="${basedir}/js.json">
			<layout xsi:type="JsonLayout">
				<attribute name ="LOG_MESSAGE" layout="${event-properties:message}" />
			</layout>
		</target>
	</targets>
</nlog>